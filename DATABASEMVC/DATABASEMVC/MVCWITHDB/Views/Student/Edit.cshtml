@using BOL.student
@{
    ViewData["Title"]="Update";
}

 @{
    Student s=(Student)ViewData["particular_id"];

    <form method="post">
    
        <div class="form-group" >
        <label for="Student ID">Student ID</label>
        <input type="number" class="form-control" value=@s.ID readonly name="Id">
    </div>
        <div class="form-group">
        <label for="First_name">First_name</label>
        <input type="text" class="form-control" placeholder=@s.First_name name="First_name">
    </div>
        <div class="form-group">
        <label for="Last_name">Last_names</label>
       <input type="text" class="form-control" placeholder=@s.Last_name name="Last_name">
    </div>

    <input type="submit" value="Save"/>
   
    </form>
          
}
